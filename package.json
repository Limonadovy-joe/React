{
  "name": "dev-stack",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:babel": "npx babel src --out-dir dist --presets=@babel/env",
    "compile-require-module": "npx tsc --module amd src/App.ts",
    "build:ts": "npx babel src --out-dir dist --extensions \".ts,.tsx\" --watch && npm run compile-require-module",
    "clean-dist": "rmdir /S dist && mkdir dist",
    "require-hook": "node -r @babel/register ./node_modules/webpack/bin/webpack",
    "lol": "npm run require-hook",
    "start": "set NODE_ENV=development | webpack  serve --hot --config webpack.dev.js",
    "lol3": "npm run require-hook && npm run clean-dist",
    "dev": "set NODE_ENV=production | webpack --config webpack.dev.js",
    "lol2": "npm run require-hook && npm run clean-dist",
    "build": "set NODE_ENV=production | webpack --config webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"src/**\"",
    "lint-fix": "eslint --fix"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^4.5.1",
    "mini-css-extract-plugin": "^1.3.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "npx": "^10.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "description": ""
}
